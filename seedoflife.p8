function _init()
    icrops()
    imoney()
    iplr()
    idays()
    itrans()
    state="play"
end

function _update()
if state=="play" then
    uplr()
    uactions()
    udays()
elseif state=="dead" then
    if btnp(ðŸ…¾ï¸) then
        run()
    end
end
end

function _draw()
    cls()
    if state=="play" then
    map()
    dplr()
    dtrans()
    dhud()
    elseif state=="dead" then
        print("bot is dead, restart:ðŸ…¾ï¸",10,50,10)
        print("revenue: $"..revenue,30,70,10)
    end

end
